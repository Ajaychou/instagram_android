<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.demoapplication.fragments.dashboardmaincamera"
        android:label="Dashboard"
        tools:layout="@layout/dashboardmaincameralayout" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.demoapplication.fragments.CameraFragment"
        android:label="Camera"
        tools:layout="@layout/cameralayout" />
    <fragment
        android:id="@+id/navigation_like"
        android:name="com.example.demoapplication.fragments.ProfilePhotoesAndVideos"
        android:label="Photoes"
        tools:layout="@layout/profilephotoesandvideos" />
    <fragment
        android:id="@+id/navigation_plus"
        android:name="com.example.demoapplication.fragments.CameraFragment"
        android:label="Videos"
        tools:layout="@layout/emailadress"/>
    <fragment
        android:id="@+id/navigation_circle"
        android:name="com.example.myapplication.ProfileActivity"
        android:label="Profile"
        tools:layout="@layout/fragmentlayout"/>

</navigation>